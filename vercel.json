{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers":{
            "Acess-Control-Allow-Origin":"*"
        }
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ]
}
  